<div class="container">
  <div class="row">
    <div class="col-12">
      <% form_with model: @reshipe, url: new_user_reshipe_path do |f| %>
        <%= f.text_field :title %>
        <%= f.file_field :image, accept: "image/*" %>
        <%= f.text_area :body %>
      <% end %>
    </div>
  </div>
</div>
